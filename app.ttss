/*opacity*/
.opacity-90{
  opacity: 0.9;
  filter:Alpha(opacity=90); /* IE8 以及更早的浏览器 */
}
.opacity-80{
  opacity: 0.8;
  filter:Alpha(opacity=80); /* IE8 以及更早的浏览器 */
}
.opacity-70{
  opacity: 0.7;
  filter:Alpha(opacity=70); /* IE8 以及更早的浏览器 */
}
.opacity-60{
  opacity: 0.6;
  filter:Alpha(opacity=60); /* IE8 以及更早的浏览器 */
}
.opacity-50{
  opacity: 0.5;
  filter:Alpha(opacity=50); /* IE8 以及更早的浏览器 */
}
.opacity-40{
  opacity: 0.4;
  filter:Alpha(opacity=40); /* IE8 以及更早的浏览器 */
