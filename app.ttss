/* 页面开始 */
#app{
  background-color: #FAFAFC;
  color: #333333;
  font-size:24rpx;
}
.flex-fill-100{
  -ms-flex: 1 1 100% !important;
  -webkit-box-flex: 1 !important;
  flex: 1 1 100% !important;
}
.hr{
  height: 1px;
  background: rgba(216,216,216,1);
  margin:30rpx 0;
}
.tt-checkbox-wrapper .tt-checkbox-input{
  border-radius: 0!important;
}
/* 精选推荐 */
.dy_recommend .item_img{
  width:160rpx;
  height:160rpx;
  border-radius:14rpx;
}
.dy_recommend .item_info{
  width:72%;
}
.dy_recommend .item_test_btn{
  color: #40A9FF;
  background:rgba(64,169,255,0.2);
  border-radius:30rpx;
  margin: 0;
  width:152rpx;
}
.dy_recommend .item_test_btn:after{
  border:0
}
.dy_recommend .hot_icon{
  width:24rpx;
  height: 24rpx;
  color: #FF323E;
}
.dy_recommend .testNum{
  max-width:196rpx;
}
.dy_recommend .testNum2{
  max-width:340rpx;
}
/* 图片加载失败 */
.test_img_error{
  left:50%;
  top:50%;
  transform: translate(-50%,-50%);
  text-align: center;
}
.test_img_error icon{
  font-size:300rpx;
  color: #ccc;
}